      <definition>
        <class_type>TRANSFORM_CLASS</class_type>
        <class_name>Processor</class_name>
        <class_source>
String full_nameField;
String short_nameField;
String cn_nameField;
String organization_codeField;
String regis_dateField;
String establish_dateField;
String product_codeField;
String currencyField;
String custodian_nameField;
String company_nameField;
String run_stateField;

com.trumgu.kettle.utils.SnowflakeDistributeId snowflake = new com.trumgu.kettle.utils.SnowflakeDistributeId(0,0);




public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
    // First, get a row from the default input hop
	//
	Object[] r = getRow();
    // If the row object is null, we are done processing.
	//
	if (r == null) {
		setOutputDone();
		return false;
	}
	// 获取html数据
	String result = get(Fields.In, "result").getString(r);
	// 使用Jsoup解析html
	org.jsoup.nodes.Document doc = org.jsoup.Jsoup.parse(result);

	org.jsoup.select.Elements field_1 =  doc.selectXpath("//*[text()='基金名称:']/following-sibling::td[1]");
	org.jsoup.select.Elements field_3 =  doc.selectXpath("//*[text()='基金编号:']/following-sibling::td[1]");
    org.jsoup.select.Elements field_4 =  doc.selectXpath("//td[text()=\"组织机构代码\"]/following-sibling::td");
    org.jsoup.select.Elements field_5 =  doc.selectXpath("//*[text()='备案时间:']/following-sibling::td[1]");
    org.jsoup.select.Elements field_6 =  doc.selectXpath("//*[text()='成立时间:']/following-sibling::td[1]");
    org.jsoup.select.Elements field_7 =  doc.selectXpath("//*[text()='币种:']/following-sibling::td[1]");
    org.jsoup.select.Elements field_8 =  doc.selectXpath("//*[text()='托管人名称:']/following-sibling::td[1]");
	org.jsoup.select.Elements field_9 =  doc.selectXpath("//*[text()='基金管理人名称:']/parent::tr/td[2]/a");
	org.jsoup.select.Elements field_10 =  doc.selectXpath("//*[text()='运作状态:']/following-sibling::td[1]");




	// Let's look up parameters only once for performance reason.
	// 相当于字段映射
	if (first) {
		full_nameField = getParameter("FULL_NAME");
		short_nameField = getParameter("SHORT_NAME");
		product_codeField = getParameter("PRODUCT_CODE");
		establish_dateField = getParameter("ESTABLISH_DATE");
		regis_dateField = getParameter("REGIS_DATE");
		currencyField = getParameter("CURRENCY");
		custodian_nameField = getParameter("CUSTODIAN_NAME");
		company_nameField = getParameter("COMPANY_NAME");
		run_stateField = getParameter("RUN_STATE");
	    first=false;
	}

    // It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
	// 获取字段的值
    Object[] outputRow = createOutputRow(r, data.outputRowMeta.size());




	// Set the value in the output field
	// 把值映射到指定字段上
	String full_name = String.valueOf(field_1.text());
	String short_name = String.valueOf(field_1.text());
	String product_code = String.valueOf(field_3.text());
	String regis_date = String.valueOf(field_5.text());
	String establish_date = String.valueOf(field_6.text());
	String currency = String.valueOf(field_7.text());
	String custodian_name = String.valueOf(field_8.text());
	String company_name = String.valueOf(field_9.text());
	String run_state = String.valueOf(field_10.text());


    get(Fields.Out, full_nameField).setValue(outputRow, full_name);
	get(Fields.Out, short_nameField).setValue(outputRow, short_name);

	get(Fields.Out, product_codeField).setValue(outputRow, product_code);
	get(Fields.Out, regis_dateField).setValue(outputRow, regis_date);
	get(Fields.Out, establish_dateField).setValue(outputRow, establish_date);
	get(Fields.Out, currencyField).setValue(outputRow, currency);
	get(Fields.Out, custodian_nameField).setValue(outputRow, custodian_name);
	get(Fields.Out, company_nameField).setValue(outputRow, company_name);
	get(Fields.Out, run_stateField).setValue(outputRow, run_state);
    // putRow will send the row on to the default output hop.
    putRow(data.outputRowMeta, outputRow);

	return true;
}</class_source>
