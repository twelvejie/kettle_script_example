    <jsScripts>
      <jsScript>
        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script here
var max_time
var min_time

max_time = str2date(max_time,"yyyy-MM-dd")
min_time = str2date(min_time,"yyyy-MM-dd")

var days = dateDiff(min_time,max_time,"d")

var end_list = ''
var start_list = ''


for(var i=1; i&lt;(days/STEP_DAY)+1;i++){
	end_time = dateAdd(min_time, "d", STEP_DAY*i);
	end_list = end_list.concat(date2str(end_time,'yyyy-MM-dd')+'+'+ i,',')
	start_time = dateAdd(end_time, "d", -STEP_DAY);
	start_list = start_list.concat(date2str(start_time,'yyyy-MM-dd')+'+'+ i,',')
}

</jsScript_script>
      </jsScript>
